{
	"name": "gangangan",
	"version": "0.1.0",
	"private": true,
	"description": "できるか知らんけどガントチャート的なものを表示・編集したい。",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sk-0520/gangangan.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/sk-0520/gangangan/issues"
	},
	"homepage": "https://github.com/sk-0520/gangangan#readme",
	"scripts": {
		"test:script": "jest --runInBand",
		"test": "npm-run-all --sequential test:script",
		"dev": "next dev",
		"dev-win": "set NODE_OPTIONS=--inspect && next dev",
		"lint": "next lint",
		"build": "next build",
		"start": "next start",
		"check": "ncu",
		"update": "ncu -u && npm install && npm run license"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"js"
		],
		"transform": {
			"^.+\\.ts$": [
				"ts-jest",
				"tsconfig.json"
			]
		},
		"testMatch": [
			"**/test/scripts/**/*"
		],
		"collectCoverage": true,
		"coverageDirectory": "coverage"
	},
	"dependencies": {
		"@next/font": "13.1.6",
		"@types/node": "18.14.0",
		"@types/react": "18.0.28",
		"@types/react-dom": "18.0.11",
		"eslint": "8.34.0",
		"eslint-config-next": "13.1.6",
		"next": "13.1.6",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.43.1",
		"react-tabs": "^6.0.0",
		"typescript": "4.9.5"
	},
	"devDependencies": {
		"@types/jest": "^29.4.0",
		"jest": "^29.4.3",
		"jest-environment-jsdom": "^29.4.3",
		"npm-run-all": "^4.1.5",
		"reset-css": "^5.0.1",
		"sass": "^1.58.3",
		"ts-jest": "^29.0.5"
	}
}
